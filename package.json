{
  "name": "icedfrisby-nock",
  "description": "Concise support for mock requests in IcedFrisby",
  "keywords": [
    "frisby",
    "icedfrisby",
    "api",
    "api-testing",
    "mock"
  ],
  "version": "1.1.0",
  "license": "MIT",
  "main": "icedfrisby-nock.js",
  "scripts": {
    "coverage": "istanbul cover _mocha '**/*.spec.js'",
    "lint": "eslint '**/*.js'",
    "prettier": "prettier --write \"**/*.{md,js}\"",
    "prettier-check": "prettier-check \"**/*.{md,js}\"",
