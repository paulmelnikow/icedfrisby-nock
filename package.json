    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "icedfrisby": "^1.2.0",
    "istanbul": "^0.4.5",
    "mixwith": "^0.1.1",
    "mocha": "^5.2.0",
    "nock": "11.0.0-beta.6",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.3",
    "prettier-check": "^2.0.0"
